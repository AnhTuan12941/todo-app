      </Compile>
      <Compile Update="view\RegisterForm.cs">
        <SubType>Form</SubType>
      </Compile>
      <Compile Update="view\Form1.cs">
        <SubType>Form</SubType>
      </Compile>
    </ItemGroup>

</Project>